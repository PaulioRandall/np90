{
	"author": "Paul Williams",
	"name": "np90",
	"description": "P90 with Node file processing and out of the box Svelte preprocessing support.",
	"keywords": [
		"css",
		"minimalist",
		"processor",
		"preprocessor",
		"node",
		"svelte"
	],
	"version": "0.25.0",
	"exports": {
		".": "./src/index.js",
		"./svelte": "./src/svelte.js",
		"./util": "./src/util.js"
	},
	"type": "module",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PaulioRandall/np90.git"
	},
	"scripts": {
		"fmt": "npx prettier --loglevel warn --write --plugin-search-dir=. .",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --silent --runInBand",
		"debug": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
		"commit": "npm run fmt && npm run test && cat ./scripts/youre-ready-to-integrate.txt"
	},
	"devDependencies": {
		"jest": "29.6.2"
	},
	"dependencies": {
		"p90": "0.24.0"
	}
}
